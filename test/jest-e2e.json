{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^test/(.*)$": "<rootDir>/test/$1"
  },
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": [
      "ts-jest",
      {
        "isolatedModules": true
      }
    ]
  },
  "setupFilesAfterEnv": [
    "./test/setup.ts"
  ],
  "globalTeardown": "./test/testDb.ts"
}